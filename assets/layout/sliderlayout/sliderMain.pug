include ../../variables/variables
block swiper
	div(class='swiper' + indexTitles.index)
		block swiper-wrapper
			div(class='swiper-wrapper' + indexTitles.index + '__wrapper')
				block swiper-slide
					div(class='swiper-slide' + indexTitles.index + '__slide')
						h1 1
					div(class='swiper-slide' + indexTitles.index + '__slide')
						h1 2
					div(class='swiper-slide' + indexTitles.index + '__slide')
						h1 3
		block swiper-pagination
			.swiper-pagination
		block swiper-button-prev
			.swiper-button-prev
		block swiper-button-next
			.swiper-button-next
		block swiper-scrollbar
			.swiper-scrollbar
